{
  "name": "@elio/db",
  "version": "1.0.0",
  "description": "Database layer for Elio OS (Supabase)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./repositories": {
      "types": "./dist/repositories/index.d.ts",
      "import": "./dist/repositories/index.js"
    },
    "./factory": {
      "types": "./dist/factory.d.ts",
      "import": "./dist/factory.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@elio/shared": "workspace:*",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  }
}
