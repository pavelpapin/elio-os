    "./exa": "./dist/exa/index.js",
    "./github": "./dist/github/index.js",
    "./gmail": "./dist/gmail/index.js",
    "./grok": "./dist/grok/index.js",
    "./groq": "./dist/groq/index.js",
    "./linkedin": "./dist/linkedin/index.js",
    "./n8n": "./dist/n8n/index.js",
    "./notebooklm": "./dist/notebooklm/index.js",
    "./notion": "./dist/notion/index.js",
    "./perplexity": "./dist/perplexity/index.js",
    "./scrapedo": "./dist/scrapedo/index.js",
